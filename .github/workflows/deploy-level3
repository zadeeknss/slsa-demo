# Name of this GitHub Actions workflow.
name: Deploy: Signed container


on:
  workflow_dispatch:

jobs:
  Deploy:
    name: Deploy
    runs-on: [self-hosted, linux, slsa]
    steps:

    - name: setup cosign
      uses: sigstore/cosign-installer@v3.1.1

    - name: Check install!
      run: cosign version
        
      
